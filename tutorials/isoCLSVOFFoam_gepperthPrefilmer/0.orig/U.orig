FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
    front
    {
        type            fixedValue;
        value           uniform (1 0 0);
    }
    back
    {
        type            fixedValue;
        value           uniform (1 0 0);
    }
    prefilmer
    {
        type            noSlip;
    }
    prefilmerInlet
    {
        type            fixedValue;
        value           uniform (0.1 0 0);
    }
    topAndBottom
    {
        type            empty;
    }
    left
    {
        type            slip;
    }
    right
    {
        type            slip;
    }
    defaultFaces
    {
        type            empty;
    }
}


