FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water air);

water
{
    transportModel  Newtonian;
    nu              2.3e-06;
    rho             770;
}

air
{
    transportModel  Newtonian;
    nu              1.5e-05;
    rho             1.2;
}

sigma            0.0275;
